{
    "name": "primavera/primavera",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jhonatan Teixeira",
            "email": "jhonatan.teixeira@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "packages/persistence/src/helpers.php"
        ],
        "psr-4": {
            "Primavera\\Container\\": "packages/container/src\\",
            "Primavera\\Cache\\": "packages/cache/src\\",
            "Primavera\\Data\\": "packages/data/src\\",
            "Primavera\\Event\\": "packages/event/src\\",
            "Primavera\\Framework\\": "packages/framework/src\\",
            "Primavera\\Log\\": "packages/logger/src\\",
            "Primavera\\Metadata\\": "packages/metadata/src\\",
            "Primavera\\Persistence\\": "packages/persistence/src\\",
            "Primavera\\Http\\": "packages/http/src\\",
            "Primavera\\Doctrine\\": "packages/doctrine/src\\",
            "Primavera\\Swoole\\": "packages/swoole/src\\"
        }
    },
    "require": {
        "doctrine/annotations": "^2",
        "doctrine/cache": "^1",
        "doctrine/dbal": "^4.0",
        "friendsofphp/proxy-manager-lts": "^1",
        "guzzlehttp/guzzle": "^7.3",
        "monolog/monolog": "^2.2",
        "opis/closure": "^3.6",
        "php": ">=8.2",
        "psr/cache": "^1",
        "psr/container": "^1.0",
        "psr/event-dispatcher": "^1",
        "psr/simple-cache": "^1",
        "slim/psr7": "^1.4",
        "slim/slim": "^4",
        "symfony/cache": ">=3.3",
        "symfony/finder": "^5.1",
        "symfony/yaml": "^3",
        "doctrine/orm": "^3.0",
        "openswoole/core": "^22.1",
        "doctrine/migrations": "^3.7.4"
    },
    "require-dev": {
        "mayflower/php-codebrowser": "^4",
        "phing/phing": "^2",
        "phpmd/phpmd": "^2",
        "phpspec/prophecy": "^1.13",
        "phpunit/phpunit": "^9.4",
        "predis/predis": "^1.1",
        "sebastian/phpcpd": "^6",
        "squizlabs/php_codesniffer": "^2",
        "symplify/monorepo-builder": "*",
        "openswoole/ide-helper": "^22.1.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Primavera\\Metadata\\Test\\": "packages/metadata/tests\\"
        }
    },
    "replace": {
        "primavera/cache": "self.version",
        "primavera/data": "self.version",
        "primavera/event": "self.version",
        "primavera/framework": "self.version",
        "primavera/logger": "self.version",
        "primavera/metadata": "self.version",
        "primavera/persistence": "self.version",
        "primavera/container": "self.version",
        "primavera/http": "self.version",
        "primavera/doctrine": "self.version",
        "primavera/swoole": "self.version"
    }
}
