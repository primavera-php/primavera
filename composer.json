{
    "name": "primavera/primavera",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jhonatan Teixeira",
            "email": "jhonatan.teixeira@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "packages/persistence/src/helpers.php"
        ],
        "psr-4": {
            "Primavera\\Container\\": "packages/container/src\\",
            "Vox\\Cache\\": "packages/cache/src\\",
            "Vox\\Data\\": "packages/data/src\\",
            "Vox\\Event\\": "packages/event/src\\",
            "Vox\\Framework\\": "packages/framework/src\\",
            "Vox\\Log\\": "packages/logger/src\\",
            "Vox\\Metadata\\": "packages/metadata/src\\",
            "Vox\\Persistence\\": "packages/persistence/src\\"
        }
    },
    "require": {
        "doctrine/annotations": "^1",
        "doctrine/cache": "^1",
        "doctrine/dbal": "~3.5.0",
        "friendsofphp/proxy-manager-lts": "^1",
        "guzzlehttp/guzzle": "^7.3",
        "monolog/monolog": "^2.2",
        "opis/closure": "^3.6",
        "php": ">=8.1",
        "psr/cache": "^1",
        "psr/container": "^1.0",
        "psr/event-dispatcher": "^1",
        "psr/simple-cache": "^1",
        "slim/psr7": "^1.4",
        "slim/slim": "^4",
        "symfony/cache": ">=3.3",
        "symfony/finder": "^5.1",
        "symfony/yaml": "^3"
    },
    "require-dev": {
        "mayflower/php-codebrowser": "^4",
        "phing/phing": "^2",
        "phpmd/phpmd": "^2",
        "phpspec/prophecy": "^1.13",
        "phpunit/phpunit": "^9.4",
        "predis/predis": "^1.1",
        "sebastian/phpcpd": "^6",
        "squizlabs/php_codesniffer": "^2",
        "symplify/monorepo-builder": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Vox\\Metadata\\Test\\": "packages/metadata/tests\\"
        }
    },
    "replace": {
        "vox/cache": "self.version",
        "vox/data": "self.version",
        "vox/event": "self.version",
        "vox/framework": "self.version",
        "vox/logger": "self.version",
        "vox/metadata": "self.version",
        "vox/persistence": "self.version",
        "primavera/container": "self.version"
    }
}
